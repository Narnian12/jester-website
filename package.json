{
  "name": "jester-website",
  "description": "Vue.js 3 TypeScript / Vite wedding website template.",
  "version": "0.0.0-semantic-release",
  "license": "MIT",
  "repository": "https://github.com/Narnian12/jester-website",
  "engines": {
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.0.5",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "vite build",
    "predeploy": "pnpm run build",
    "deploy": "gh-pages -d dist",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "test:unit": "jest",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "@quasar/extras": "^1.17.0",
    "pinia": "^2.3.1",
    "quasar": "^2.18.6",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@fullhuman/vue-cli-plugin-purgecss": "^5.0.0",
    "@kouts/eslint-config": "^0.0.14",
    "@quasar/vite-plugin": "^1.10.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.25",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/compiler-sfc": "^3.5.25",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "autoprefixer": "^10.4.22",
    "babel-jest": "^29.7.0",
    "eslint": "^9.39.1",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.7.3",
    "sass-embedded": "^1.93.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vue-tsc": "^2.2.12"
  }
}
